{
    "name": "vanta/b2pos-soap-client",
    "description": "Soap client for b2pos",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Kaufman",
            "email": "d.kaufman@pos-credit.ru"
        },
        {
            "name": "Vanta Team",
            "homepage": "https://vanta.ru"
        }
    ],
    "keywords": [
        "b2pos client",
        "b2pos soap client",
        "poscredit client",
        "poscredit"
    ],
    "autoload": {
        "psr-4": {
            "Vanta\\Integration\\B2posSoapClient\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "psr/http-client": "^1.0",
        "yiisoft/http": "^1.2",
        "guzzlehttp/psr7": "^2.6",
        "symfony/serializer": "^6.3",
        "symfony/property-access": "^6.3",
        "doctrine/annotations": "^2.0",
        "brick/phonenumber": "^0.5.0",
        "moneyphp/money": "^4.2",
        "phpdocumentor/reflection-docblock": "^5.3"
    },
    "config": {
        "platform": {
            "php": "8.1.0"
        },
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.26",
        "phpstan/phpstan": "^1.10",
        "symfony/http-client": "^6.3",
        "nyholm/psr7": "^1.8",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-symfony": "^1.3"
    }
}
